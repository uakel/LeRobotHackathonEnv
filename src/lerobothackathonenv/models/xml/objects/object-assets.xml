<asset>
<mesh file="objects/bottle.stl" name="bottle_mesh"/>
<mesh file="objects/can.stl" name="can_mesh"/>
<mesh file="objects/bread.stl" name="bread_mesh" scale="0.8 0.8 0.8"/>
<mesh file="objects/cereal.stl" name="cereal_mesh"/>
<mesh file="objects/lemon.stl" name="lemon_mesh" scale="1.5 1 1"/>
<mesh file="objects/milk.stl" name="milk_mesh" scale="0.9 0.9 0.9"/>

<texture type="skybox" file="glass.png" name="tex-glass"/>
<texture file="bread.png" type="2d" name="tex-bread"/>
<texture file="soda.png" name="tex-can" type="2d"/>
<texture type="2d" file="cereal.png" rgb1="1 1 1" name="tex-cereal"/>
<texture file="light-wood.png" type="cube" name="light-wood"/>
<texture file="dark-wood.png" type="cube" name="dark-wood"/>
<texture file="brass-ambra.png" type="cube" name="metal"/>
<texture file="lemon.png" type="2d" name="tex-lemon"/>
<texture file="ceramic.png" name="tex-ceramic-new" type="2d"/>
<texture file="red-wood.png" type="cube" name="red-wood"/>
<texture file="steel-scratched.png" type="cube" name="steel-metal"/>
<texture file="brass-ambra.png" type="cube" name="brass-metal"/>

<material name="glass" reflectance="0.5" texrepeat="5 5" texture="tex-glass" texuniform="true"/>
<material name="bread" reflectance="0.7" texrepeat="15 15" texture="tex-bread" texuniform="true"/>
<material name="coke" reflectance="0.7" texrepeat="5 5" texture="tex-can" texuniform="true"/>
<material name="cereal" reflectance="0.5" texrepeat="1 1" texture="tex-cereal" texuniform="false"/>
<material name="MatMetal" texture="metal" specular="1" shininess="0.3" rgba="0.9 0.9 0.9 1"/>
<material name="MatDarkWood" texture="dark-wood" texrepeat="3 3" specular="0.4" shininess="0.1"/>
<material name="MatLightWood" texture="light-wood" texrepeat="3 3" specular="0.4" shininess="0.1"/>
<material name="lemon" reflectance="0.7" texrepeat="60 50" texture="tex-lemon" texuniform="true"/>
<material name="ceramic" reflectance="0.5" texrepeat="1 1" texture="tex-ceramic-new" texuniform="true"/>
<material name="plate_mat" texture="red-wood" texrepeat="3 3" specular="0.4" shininess="0.1"/>
<material name="smetal" reflectance="1.0" shininess="1.0" specular="1.0" texrepeat="1 1" texture="steel-metal" texuniform="true"/>
<material name="bmetal" reflectance="1.0" shininess="1.0" specular="1.0" texrepeat="1 1" texture="brass-metal" texuniform="true"/>

</asset>
